%card dimensions and borders
%!
/pageheight 8 inch def
/pagewidth 10.5 inch def
/pageborder 8.5 inch pageheight sub 2 div def
/cardwidth pageheight 9 mul 32 div def
/cardheight pageheight cardwidth 2 mul sub def

/cardborders {
 gsave
 0 1 9 {
  cardstart
  newpath
  1 1 moveto
  1 cardheight 1 sub lineto
  cardwidth 1 sub cardheight 1 sub lineto
  cardwidth 1 sub 1 lineto
  closepath
  stroke
  cardend
 } for
 grestore
} def

/cardxs 10 array def

%put x-offsets into cardxs
0 1 1{
 /index1 exch def
  0 1 2 {
  /index2 exch def
   cardxs  index1 3 mul index2 add
     pageborder cardheight index2 mul add   put  
  } for
} for 
0 1 3 {
 /index1 exch def
 cardxs  6 index1 add
  pageborder cardwidth index1 mul add   put
} for

/cardys 10 array def 
0 1 1 {
  /index1 exch def
  0 1 2 {
   /index2 exch def
   cardys  index1 3 mul index2 add
     pageborder pageheight add cardwidth index1 mul sub  put
  } for
} for
0 1 3 {
  /index1 exch def
  cardys  6 index1 add
  pageborder  put
} for

/cardrs   [-90  -90  -90  -90  -90  -90    0    0    0    0   ] def

/cardstart {
 gsave
 /cardnum exch def
 cardxs cardnum get cardys cardnum get translate
 cardrs cardnum get rotate
 0 0 moveto
} def

/cardend {
  grestore
} def


