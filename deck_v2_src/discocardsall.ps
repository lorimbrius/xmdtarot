/arctoalt { %see code for examples: 
 %essentially takes three items and two already on stack
 /y2 exch def
 /x2 exch def
 /r exch def
 /y1 exch def
 /x1 exch def
 r 0 eq {
  x1 y1 lineto
 }{
  x1 y1 x2 y2 r arcto
  4 {pop} repeat
 } ifelse
 x2 y2 
} def

/arcbetween { %given 2 points and r, arcs between
 /r exch def
 /y2 exch def
 /x2 exch def
 /y1 exch def
 /x1 exch def
 /direction true def
 r 0 lt {
  /r r -1 mul def
  /direction false def
 } if
 /d x1 x2 sub 2 exp y1 y2 sub 2 exp add sqrt def
 r d 2 div lt { %use semicircle if r too small
  /r d 2 div def
 } if
 /sideangle d r 2 mul div acos def
 /arcangle 90 sideangle sub 2 mul def
 /lineangle y2 y1 sub x2 x1 sub atan def
 %correct for range? possibly not.
 direction {
  %direction is true. arc is counterclockwise.
  /startangle lineangle sideangle add 180 add def
  x1 startangle 180 sub cos r mul add 
  y1 startangle 180 sub sin r mul add
  r startangle startangle arcangle add arc
 } {
  %direction is false. arc is clockwise.
  /startangle lineangle sideangle sub 180 add def
  x1 startangle 180 sub cos r mul add 
  y1 startangle 180 sub sin r mul add
  r startangle startangle arcangle sub arcn
 } ifelse 
} def

/arcbetweento {
 /r exch def
 /y2 exch def
 /x2 exch def
 currentpoint x2 y2 r arcbetween
} def

/acos {
 /cosa exch def
 cosa abs 1 eq{
  cosa 0 gt {
   0
  }{
   180
  } ifelse 
 }{
  90 cosa 1 cosa 2 exp sub sqrt div 1 atan sub
 } ifelse
} def


