% ellipse library
/ellipsedict 9 dict def	
ellipsedict /mtrx matrix put
/ell {
        ellipsedict begin
	/endangle exch def
	/startangle exch def
	/yrad exch def
	/xrad exch def
	/y exch def
	/x exch def
	/savematrix mtrx currentmatrix def
	x y translate
	xrad yrad scale
	0 0 1 startangle endangle arc
	savematrix setmatrix
        end
} def
/elln {
        ellipsedict begin
	/endangle exch def
	/startangle exch def
	/yrad exch def
	/xrad exch def
	/y exch def
	/x exch def
	/savematrix mtrx currentmatrix def
	x y translate
	xrad yrad scale
	0 0 1 startangle endangle arcn
	savematrix setmatrix
        end
} def

